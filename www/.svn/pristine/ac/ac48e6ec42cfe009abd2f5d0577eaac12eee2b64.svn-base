<!doctype html>
<html>
<head>
<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
		<meta name="format-detection" content="telephone=no">
		<meta name="renderer" content="webkit">
		<meta http-equiv="Cache-Control" content="no-siteapp" />
		<script src="js/include.js"></script>
		<link href="icon/iconfont.css" rel="stylesheet" type="text/css">
		<link href="css/star-rating.css" media="all" rel="stylesheet" type="text/css">
		<link href="css/style.css" rel="stylesheet" type="text/css" />
		<link href="icon/iconfont.css" rel="stylesheet" type="text/css">
		<link href="css/style.css" rel="stylesheet" type="text/css">
<style type="text/css">
 
    body,
    html {
        width: 100%;
        height: 100%;
    }
    
    .line {
        border: none;
        border-top: 1px solid #b8b1a4;
        height: 1px;
        width: 100%;
        margin-top: 1em;
    }
    
    .mi-black {
        color: #4489ce;
        background: white;
        outline: none;
        border: white 1px solid;
        border-top: #dddddd 1px solid;
    }
    
    .line1 {
        border: none;
        border-top: 1px solid #ccc;
        height: 1px;
        width: 100%;
        margin-top: 0.3em;
        margin-bottom: 0.5em;
    }
    
    #tcColor {
        color: #333333;
    }
    
    #testColor {
        color: #333333;
        font-size: 15px;
    }
    
    .am-offcanvas-bar {
        position: fixed;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        z-index: 1091;
        width: 75%;
        max-width: 100%;
        background: #f8f8f8;
        border: none;
        overflow-y: auto;
        /* scrollable */
        -webkit-overflow-scrolling: touch;
        /* scrollable */
        -webkit-transition: -webkit-transform 0.3s ease-in-out;
        transition: transform 0.3s ease-in-out;
        -webkit-transform: translateX(-100%);
        -ms-transform: translateX(-100%);
        transform: translateX(-100%);
    }
    
    .am-offcanvas-bar:after {
        content: "";
        display: block;
        position: absolute;
        top: 0;
        bottom: 0;
        right: 0;
        width: 1px;
        background: transparent;
    }
    
    .am-fr {
        float: left;
    }
    
    .am-header-fixed {
        position: absolute;
    }
    
    .am-slider-d1 {
        -webkit-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.2);
        box-shadow: 0 1px 4px rgba(0, 0, 0, 0.2);
        /* Direction Nav */
    }
    
    .am-slider {}
    
    .loading .am-slider-d1 .am-viewport {
        max-height: 300px;
    }
    
    .am-slider-d1 .am-slider-desc {
        padding: 8px 35px;
        width: 100%;
        color: black;
        background-color: white;
    }
    
    .am-topbar-inverse {
        background-color: #f5f3f4;
        border-color: #f5f3f4;
    }
    
    .am-navbar .am-navbar-nav {
        background-color: #f5f3f4;
    }
    
    .am-modal-actions-group {
        margin: 0px;
    }
    
    .img {}
    
    .img1 {}
    
    .data-am-selected {
        width: 33%;
        float: left;
    }
    
    .icon-sousuo {
        float: left;
        margin-top: 2px;
        color: #fff;
        margin-left: 20px;
        margin-right: 10px;
    }
    
    .icon-dingwei {
        color: #fff;
        font-size: 2rem;
    }
    
    #dropdown {
        width:50%;
        height: 2em;
        border-top: none;
        border-bottom: none;
    }
    
    #dropdown ul {
        width: 100%;
        background-color: white;
        position: absolute;
        display: none;
        z-index: 100;
        margin: 0;
        margin-left: -50%;
    }
    
    #dropdown1 {
        width: 50%;
        height: 2em;
    }
    
    #dropdown1 ul {
        width: 100%;
        background-color: white;
        position: absolute;
        display: none;
        z-index: 100;
        margin: 0;
        margin-left: -50%;
    }
   
    
    li {
        list-style-type: none;
    }
    .icon-ding{
    	float:left;
    	margin-right:5px;
    	color:#ff9402
    }
    .am-u-sm-8 .am-g {
    	height: 1.5em;line-height: 1.5em;color:#666666
    }
    .am-u-sm-8 .am-g span{
    	font-size: 12px;
    }
</style>
</head>
<body>

	<header data-am-widget="header" class="am-header am-header-default am-header-fixed mi-title">
		<div class="am-header-left am-header-nav">
            <a href="javascript:mi_exit(-1);" style="text-align: left">
                <i class="iconfont icon-fanhuijiantou1"></i>
            </a>
        </div>
        <div style="margin-left:2em;margin-right:3rem;line-height:35px;background:white;overflow:hidden;border-radius: 30px;margin-top:6px" onclick="">
            <i class="iconfont icon-sousuo" style="color: lightgray"></i>
            <input type="text" id="search1" name="search1" maxlength="15" required placeholder="输入要搜索的内容" style="border: none;" />
        </div>
        <div class="am-header-right am-header-nav">
        <input type="submit" onclick="search()" value="查询" style="float: right;background-color:#4384c6;color: white;border: none;outline: none;line-height:1.8em;height: 1.8em;margin-top:8px;padding:0 0.8em;border-radius: 30px" />
		</div>
	</header>
	
	
	
	<div class="am-g" style="position:relative; padding-top: 0.5em;background-color: white;border-bottom:1px solid #e5e5e5">	
        <div id="dropdown" style="z-index: 100;float: left;  ">
            <p style="width: 50%;float: left;margin:0;margin-left: 1.5em;text-align: center;color:#666666" id = "quanbu">全部</p>
            <i class="am-selected-icon am-icon-caret-down" style="color: #666666;"></i>
            
        </div>
        <div id="dropdown1" style="z-index: 100;float: left;  ">
            <p style="width: 50%;float: left;margin:0;margin-left: 1.5em;text-align: center;color:#666666">智能排序</p>
            <i class="am-selected-icon am-icon-caret-down" style="color:#666666;"></i>
            <ul style="padding-left: 0;text-indent:2em;margin-top: 8px">
                <li onclick="type12('0')" style="height:3em;line-height:3em;border:1px solid #dddddd;border-left: none;
                  border-right: none;border-top: none;color: #807f85;">销售量</li>               
            </ul>
        </div>
    </div>
    <div class="am-g" style="background-color: white;height: 2.5em;line-height: 2.5em;font-size:1.5rem;padding-right: 1em;padding-left: 1em;border-bottom:1px solid #e5e5e5">
        <span style="color:#b8b8b8">当前 : 黑龙江省哈尔滨市</span><!--img src="mychongzhi.html" style="float: right;"-->
        <i class="iconfont icon-shuaxin" style="float: right;color:#b8b8b8" onclick="mychongzhi.html"></i>
    </div>

	<div id="d-shanghulist" class="am-u-sm-12">
			<script id='s-shanghulist' type="text/html">
			{{each chaxunlist as val i}}
				<div class="am-g" onclick="goToRiceDetail('{{val.shanghuID}}')" style="background-color: white;border-bottom:1px solid #e5e5e4;border-radius: 4px;">
					<div class="am-u-sm-4" style="height:8rem">
					<img style="width:100%;height:6rem;border:none;margin-top:14px;" src="{{imgurl+val.tupiandizhi}}">
					</div>
				<div class="am-u-sm-8" style="padding:0 0 0 0.5em;">
				<div style="color:#000;font-size: 14px; height: 1.5em;float:left;margin-bottom:5px;margin-top:8px">{{val.dianpumingcheng}}</div>
				<span style="font-size: 1em;color:#999999;float: right;margin-right: 10px;margin-top:10px">{{val.juli}}</span>
				<div style="line-height: 1em;clear: both;margin-bottom:5px">
				<span style="font-size: 0.8em;color: red;">{{val.jine}}元可抵扣{{val.jifen}}积分</span>			
				</div>
				<div class="am-u-sm-8" style="font-size: 0.8em;padding:0;margin-bottom:10px;">
					<div style="font-size: 1em;color:#999999;"><i class="iconfont icon-ding"></i>已售<span>{{if val.yishou == ""}}0{{else}}{{val.yishou}}{{/if}}</span></div>
				</div>
				</div>
				</div>
			{{/each}}
			</script>
		
		</div>
		<div id="modal_prov" class = "ul-fixed3" style="max-height: 60%;overflow:auto;width:100%;position: absolute;">
	        <ul id = "ul_yiji" class="am-list" style="float:left;width:50%;text-indent:2em;" hidden>
						
			</ul>
			<ul id = "ul_erji" class="am-list" style="float:right;width:50%;margin-top:0;text-indent:2em;background: white;" hidden>
						
			</ul>
        </div>
	
	<script type="text/javascript">
	var isLogin = true;
	var userid = window.localStorage.getItem('userid');
	var token = window.localStorage.getItem('token');
	var cityCode = window.localStorage.getItem('cityCode');
	var ejfenleiID = getParameter("ejfenleiID");
	var fenleiID = ""	
	var isLogin = true;
	var yishou ="";
	var cityCode = window.localStorage.getItem('cityCode');
	var city = window.localStorage.getItem('cityName');
	
	if (userid == null || userid == "null") {
		isLogin = false;
	} else {
		isLogin = true;
	}
	
	jQuery(document).ready(function() {
		$('input').keyup(function(){var oldv = $(this).val(); $(this).val(oldv.replace(/[\\\ud83d].*|[\\\ud83c].*/ig,''))});
		$('textarea').keyup(function(){var oldv = $(this).val(); $(this).val(oldv.replace(/[\\\ud83d].*|[\\\ud83c].*/ig,''))});
	});
	
	jQuery(document).ready(function() {
		fenlei();
		if(ejfenleiID != ""){
			search();
		}
	});

$("#dropdown p").click(function(){ 
		
		$("#dropdown1 ul").slideUp("fast");
		$("#dropdown2 ul").slideUp("fast");
		$("#ul_yiji").slideUp("fast");
		$("#ul_erji").slideUp("fast");
		
	    var ul = $("#ul_yiji"); 
	    if(ul.css("display")=="none"){ 
	        ul.slideDown("fast"); 
	    }else{ 
	        ul.slideUp("fast"); 
	        $("#ul_erji").slideUp("fast");
	    }
	}); 
	$("#dropdown ul li").click(function(){ 
	    var txt = $(this).text(); 
	    $("#dropdown p").html(txt); 
	    $("#ul_yiji").hide(); 
	}); 
	
	function type12(id){
		yishou = id;
		$('#quanbu').text("销售量");
		search();
	}
	$("#dropdown1 p").click(function(){ 
		
		$("#dropdown1 ul").slideUp("fast");
		$("#dropdown2 ul").slideUp("fast");
		$("#ul_yiji").slideUp("fast");
		$("#ul_erji").slideUp("fast");
		
	    var ul = $("#dropdown1 ul"); 
	    if(ul.css("display")=="none"){ 
	        ul.slideDown("fast"); 
	    }else{ 
	        ul.slideUp("fast"); 
	    }
	}); 
	$("#dropdown1 ul li").click(function(){ 
	    var txt = $(this).text(); 
	    $("#dropdown1 p").html(txt); 
	    $("#dropdown1 ul").hide(); 
	}); 
	
	function type1234(id){
		feileiID = id;
		search();
	}
	
	function search() {
		var word = $('#search1').val().trim();
		$.ajax({
			type: "post",		
			url:baseurl("APPCHAXUN.do"),
			dataType: "jsonp",
			data:{
				"cityCode": cityCode,
				"jingdu": 126.7023827709,
				"weidu" : 45.7591702951,
				"name" : word,
				"fenleiID" : fenleiID,
				"yishou" : yishou,
				"ejfenleiID" : ejfenleiID
							
			},
			jsonp: "callback",
			jsonpCallback: "fenleichaxun",
			success: function(json) {
				json.imgurl = imgurl;
				var html = template('s-shanghulist', json);
				$("#d-shanghulist").html(html);
			},
			error: function() {
				
			}
		});
	}

	function fenlei() {
		$.ajax({
			type: "post",		
			url:baseurl("APPYIJIFENLEI.do"),
			dataType: "jsonp",
			data:{
											
			},
			jsonp: "callback",
			jsonpCallback: "fenleilist",
			success: function(json) {
				$('#ul_yiji').append(
						'<li style="border:none;"><button class="am-btn am-btn-secondary am-btn-block" '+
						'style = "height:3em;background:#e8e8e8;color:#4489ce;border:none;" '+
						'onclick="proc_all()">全部</button></li>'
				);
				if(json.FenleiList.length > 0){
					for(var i=0;i<json.FenleiList.length;i++){
						$('#ul_yiji').append(
								'<li style = "color: #807f85;background:#e8e8e8;height:3em;line-height:3em;border-bottom:1px #ededed solid;border-top:none;" ' +
								'val="'+json.FenleiList[i].fenleiID+'" onclick="provitemClick(\''+json.FenleiList[i].fenleimingcheng+'\','+json.FenleiList[i].fenleiID+',this)"> ' +
								'<span style = "color: #807f85;display:block;padding-left:1em;">'+json.FenleiList[i].fenleimingcheng+'</span></li> '
						);
					}
					$('#ul_yiji').append(
							'<li style="border:none;"><button class="am-btn am-btn-secondary am-btn-block" '+
							'style = "height:3em;background:white;color:#4489ce;border:none;" '+
							'onclick="closemodal()">取消</button></li>'
					);
				}
			},
			error: function() {
				
			}
		});
	}
	function provitemClick(fenleimingcheng,fenleiID,item) {
		$('#ul_yiji li').css("background","#e8e8e8");
		$('#ul_erji').empty();
		$(item).css("background","white");
		erjiComboBox(fenleiID);		
	}
	function erjiComboBox(fenleiID) {
		
		$.ajax({
			type : "post",
			url : baseurl("APPERJFENLEI.do"),
			dataType : "jsonp",
			data:{
				"fenleiID":fenleiID
			},
			jsonp : "callback",
			jsonpCallback : "fenleilist",
			success : function(json) {
				$('#ul_erji').append(
						'<li style="border:none;"><button class="am-btn am-btn-secondary am-btn-block" '+
						'style = "height:3em;background:#fff;color:#807f85;border:none;border-bottom: 2px #ededed solid;" '+
						'onclick="yiji_all(\''+fenleiID+'\')">全部</button></li>'
				);
							
				for(var i=0;i<json.erjifenleiList.length;i++){
					$('#ul_erji').append(
							'<li style = "color: #807f85;height:3em;line-height:3em;border-bottom:2px #ededed solid;border-top:none;margin-left:8px;" ' +
							'val="'+json.erjifenleiList[i].fenleiID+'" onclick="erjiitemClick(\''+json.erjifenleiList[i].fenleimingcheng+'\','+json.erjifenleiList[i].fenleiID+',this)"> ' +
							'<span style = "color: #807f85;display:block;padding-left:1em;">'+json.erjifenleiList[i].fenleimingcheng+'</span></li>'
					);
				}
				$('#ul_erji').append(
						'<li style="border:none;"><button class="am-btn am-btn-secondary am-btn-block" '+
						'style = "height:3em;background:white;color:#4489ce;border:none;" '+
						'onclick="closemodal1()">取消</button></li> '
				);
			
				
				$('#ul_erji').show();
			},
			error : function() {
			}
		});
	}
	var cityName = "";
	function erjiitemClick(fenleimingcheng,dd,item) {
		$('#ul_erji li').css("background","white");
		
		$(item).css("background","#e8e8e8");
		fenleiID = dd;
		cityName = fenleimingcheng;
		$('#quanbu').text(cityName);
		$('#ul_erji').hide();
		$('#ul_yiji').hide();		
		search();
	}
	
	function proc_all(){
		$('#ul_yiji').hide();
		$('#ul_erji').hide();
		$('#ul_yiji li').css("background","#e8e8e8");
		$('#ul_erji li').css("background","white");
		search();
	}
	function yiji_all(cc) {
		$('#ul_yiji').hide();
		$('#ul_erji').hide();
		$('#ul_yiji li').css("background","#e8e8e8");
		$('#ul_erji li').css("background","white");
		ejfenleiID = "";
		fenleiID = cc;
		search();
	}
	
	
	function closemodal(){
		$('#ul_yiji').hide();
	}
	function closemodal1(){
		$('#ul_erji').hide();
	}
		
	</script>
</body>
</html>
