var myIcon = new BMap.Icon("img/shopLocation.png", new BMap.Size(32, 32));
var marker = new BMap.Marker(point, {
    icon: myIcon
}); // 创建标注
map.addOverlay(marker); // 将标注添加到地图中
map.panTo(point);
